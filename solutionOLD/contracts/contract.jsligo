type storage = int;
type parameter =
  ["Increment", int] 
| ["Decrement", int]
| ["Reset"];
type result_ = [list<operation>, storage];

const main = (action : parameter, store : storage) : result_ => {
  return [
    list([]),
    match(action, { 
      Increment: (n: int) => store + n,
      Decrement: (n: int) => store - n,
      Reset:     ()       => 0
    })
  ];
};

